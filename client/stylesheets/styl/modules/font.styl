@font-face {
    font-family: 'LarsseitW01';
    src: url('./fonts/hinted-LarsseitW01-Medium.eot');
    src: url('./fonts/hinted-LarsseitW01-Medium.eot?#iefix') format('embedded-opentype'),
        url('./fonts/hinted-LarsseitW01-Medium.woff2') format('woff2'),
        url('./fonts/hinted-LarsseitW01-Medium.woff') format('woff'),
        url('./fonts/hinted-LarsseitW01-Medium.ttf') format('truetype'),
        url('./fonts/hinted-LarsseitW01-Medium.svg#LarsseitW01-Medium') format('svg');
    font-weight: 500;
    font-style: normal;
}

